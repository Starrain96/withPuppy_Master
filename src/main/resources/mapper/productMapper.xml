<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="product">
	<!-- insert, delete, update는 무조건 resultType="int"이므로
	     프로그래머가 써주지 않아도 이미 resultType="int"가 
	     있는 것으로 본다.!
	 -->
	
	<select id="one" parameterType="Int" resultType="productVO">
		select * 
		from product
		where detail = #{id}
	</select>
	
	<select id="all"  resultType="productVO">
		select *
		from product
	</select>
	
	
	<select id="dogcate" parameterType="Int" resultType="productVO">
		select *
		from product where category2 = #{category2}
	</select>
	
	<!-- 
	<select id="dogcate2" parameterType="Int" resultType="productVO">
		select *
		from product where category2 >= #{cate1} and category2 <= #{cate2}
	</select> -->
	
	
</mapper>



